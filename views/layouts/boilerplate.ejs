	/* Footer design */

		  
		  a {
			text-decoration: none;
			color: #9ca3af;
			transition: color 0.3s;
		  }
		  
		  a:hover {
			color: #ffffff;
		  }
		  
		  ul {
			list-style: none;
		  }
		  
		
		  /* Footer Styles */
		  footer {
			background-color: #1a202c;
			
			color: #d1d5db;
		  }
		  
		  .footer-content {
			padding: 4rem 1rem 1.5rem;
		  }
		  
		  .footer-grid {
			display: grid;
			grid-template-columns: 1fr;
			gap: 2.5rem;
		  }
		  
		  /* About Section */
		  .footer-about {
			margin-bottom: 1rem;
		  }
		  
		  .footer-title {
			color: #ffffff;
			font-size: 1.25rem;
			font-weight: 700;
			margin-bottom: 1rem;
		  }
		  
		  .footer-about p {
			color: #9ca3af;
			margin-bottom: 1rem;
		  }
		  
		  .social-icons {
			display: flex;
			gap: 1rem;
			margin-top: 1rem;
		  }
		  
		  .social-icon {
			width: 2.5rem;
			height: 2.5rem;
			border-radius: 50%;
			display: flex;
			align-items: center;
			justify-content: center;
			transition: background-color 0.3s;
		  }
		  
		  .social-icon svg {
			width: 1.25rem;
			height: 1.25rem;
		  }
		  
		  .facebook {
			background-color: #2563eb;
		  }
		  
		  .facebook:hover {
			background-color: #1d4ed8;
		  }
		  
		  .twitter {
			background-color: #60a5fa;
		  }
		  
		  .twitter:hover {
			background-color: #3b82f6;
		  }
		  
		  .instagram {
			background-color: #db2777;
		  }
		  
		  .instagram:hover {
			background-color: #be185d;
		  }
		  
		  .linkedin {
			background-color: #1e40af;
		  }
		  
		  .linkedin:hover {
			background-color: #1e3a8a;
		  }
		  
		  /* Quick Links */
		  .footer-links h3 {
			color: #ffffff;
			font-size: 1.125rem;
			font-weight: 600;
			margin-bottom: 1rem;
		  }
		  
		  .footer-links li {
			margin-bottom: 0.5rem;
		  }
		  
		  /* Contact Info */
		  .footer-contact h3 {
			color: #ffffff;
			font-size: 1.125rem;
			font-weight: 600;
			margin-bottom: 1rem;
		  }
		  
		  .contact-item {
			display: flex;
			align-items: flex-start;
			margin-bottom: 0.75rem;
		  }
		  
		  .contact-icon {
			width: 1.25rem;
			height: 1.25rem;
			margin-right: 0.75rem;
			color: #60a5fa;
			flex-shrink: 0;
			margin-top: 0.125rem;
		  }
		  
		  /* Download App */
		  .footer-app h3 {
			color: #ffffff;
			font-size: 1.125rem;
			font-weight: 600;
			margin-bottom: 1rem;
		  }
		  
		  .footer-app p {
			color: #9ca3af;
			margin-bottom: 1rem;
		  }
		  
		  .app-buttons {
			display: flex;
			flex-direction: column;
			gap: 0.75rem;
		  }
		  
		  .app-button img {
			height: 2.5rem;
		  }
		  
		  /* Copyright */
		  .copyright {
			border-top: 1px solid #374151;
			margin-top: 3rem;
			padding-top: 2rem;
			padding-bottom: 1rem;
			text-align: center;
		  }
		  
		  .copyright-content {
			display: flex;
			flex-direction: column;
			align-items: center;
			gap: 1rem;
		  }
		  
		  .copyright-links {
			display: flex;
			flex-wrap: wrap;
			justify-content: center;
		  }
		  
		  .copyright-links a {
			margin: 0 0.75rem;
		  }
		  
		  /* Media Queries */
		  @media (min-width: 768px) {
			.footer-grid {
			  grid-template-columns: repeat(2, 1fr);
			}
			
			.copyright-content {
			  flex-direction: row;
			  justify-content: space-between;
			}
			
			.copyright-links {
			  margin-top: 0;
			}
		  }
		  
		  @media (min-width: 1024px) {
			.footer-grid {
			  grid-template-columns: repeat(4, 1fr);
			}
		  }




		
	


